        <div class="nav-obj pages-link">
          <i class="fa-solid fa-house"></i>
          <a href="http://localhost:3001/blog">Home</a>
        </div>
        <div class="nav-obj pages-link">
          <i class="fa-solid fa-feather"></i>
          <a href="#">Trending</a>
